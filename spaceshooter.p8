-- particles

function init_particles()
	explosion_p = {}
	preshoot_p = {}
end

function update_explosion(en)
	for i=1, 20 do
		local particle = {
			x=en.x,
			y=en.y,
			xsp=rnd(4)-2,
			ysp=rnd(4)-2,
			size=rnd(4),
			life=rnd(15)
		}
		add(explosion_p, particle)
	end
end

function draw_explosion()
	for p in all(explosion_p) do
		p.life += 1
		
		local colors = {7, 10, 9, 8, 2, 5}
		
		-- choose color based on life
		local index = flr(p.life / 5) + 1
		
		if index <= #colors then
		    circfill(p.x, p.y, p.size, colors[index])
		end

		p.x += p.xsp
		p.y += p.ysp
		p.xsp *= 0.7
		p.ysp *= 0.7
		
		if p.life > 30 then
   del(explosion_p, p)
  end
	end
end

function update_preshoot()
	for i=1, 5 do
		local particle = {
			x=player.x+4,
			y=player.y,
			xsp=rnd(4)-2,
			ysp=-rnd(2),
			rad=rnd(2),
			life=rnd(5)
		}
		add(preshoot_p, particle)
	end
end

function draw_preshoot()
	for p in all(preshoot_p) do
		p.life += 1
		
		local colors = {10, 9, 8}
		
		-- choose color based on life
		local index = flr(p.life / 6,6) + 1
		
		if index <= #colors then
		    circfill(p.x, p.y, p.size, colors[index])
		end

		p.x += p.xsp
		p.y += p.ysp
		p.xsp *= 0.7
		p.ysp *= 0.7
		
		if p.life > 30 then
   del(preshoot_p, p)
  end
	end
end
