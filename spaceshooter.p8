__lua__
function _init()
	invuln = 0
	old_score = 0
	score = 0
	player = {x=64, y=100, animation=1}
	state = "game"
	show_wavetext = true
	init_health()
	init_fire()
	init_starfield()
	init_enemies()
	init_shoot()
	init_particles()
	init_wavetext()
	
	frame_counter = 0
end

function _update()
	frame_counter += 1
	
	if state == "game" then
		update_health()
		moveplayer() -- movement
		update_fire() -- fire animation
		
		-- shoot
		if btnp(üÖæÔ∏è) then 
			shoot() 
		end
	
		update_enemies()
		increment_speed()
			
	end
	
end

function _draw()
	if state=="game" then
		cls(1)
		
		star_draw()
		draw_health()
		bullet_draw()
		draw_enemies()
		draw_explosion()
		draw_score(score)		
		
	
		-- draw player
		-- if invuln blink
		if invuln <= 0 then
			spr(player.animation, player.x, player.y)
		elseif sin(frame_counter/10) < 0 then
			spr(player.animation, player.x, player.y)
		end
		
		draw_fire() -- draw fire animation
		
		if show_wavetext then
			display_wavetext()
		end
		
		elseif state=="lose" then
			draw_lose()
	
	end
end


