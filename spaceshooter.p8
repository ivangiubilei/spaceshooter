-- ui
function init_health() 
	starting_health=3
	health = {}
	
	for heart=starting_health, 1, -1 do
		add(health, {x=8*heart, y=8})
	end
end

function update_health()
	if #health == 0 then
		state = "lose"
	end
end

function draw_health()
	for heart in all(health) do
		spr(32, heart.x, heart.y)
	end
end

function reduce_health()
	for h in all(health) do
		del(health, h)
		break	
	end
	
	sfx(1)
end

function draw_lose()
	cls(8)
		
	local text = "gamover!"
	print("gameover!", flr((128-#text*3)/2)-6,51, 7)
		
	text = "highscore: "
	print(text..score, flr((128-(#text+count_digits(score))*3)/2)-6, 59, 7)
	
	if btn(‚ùé) then
		_init()
	end
end

function draw_score(score_) 
	local text = "score: "
	print(text .. score_, 100-#text*3-count_digits(score)*3, 8, 7)
end

function init_wavetext()
	wave_count = 1
	wavetext_counter_open = 0
	wavetext_counter_close = 0
	show_wave = true
end

function display_wavetext()
	if show_wave then
		wavetext_counter_open += 1
		if wavetext_counter_open >= 30 then
			wavetext_counter_close += 1
			print("wave " .. wave_count, (114-count_digits(wave_count)*3)/2, flr(123/2), 7)
			if wavetext_counter_close >= 60 then
				wavetext_counter_open = 0
				wavetext_counter_close = 0
				wave_count += 1
				show_wave = false
			end
		end
	end
end
